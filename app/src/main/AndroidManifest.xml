<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".FindPwActivity"
            android:theme="@style/NoActionBarTheme"></activity>
        <activity
            android:name=".FindIDActivity"
            android:theme="@style/NoActionBarTheme"/>

        <service
            android:name=".BodyShopService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".CarKeyActivity"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name=".OpeningActivity"
            android:theme="@style/NoActionBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReservationStatusActivity"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name=".RegistActivity"
            android:theme="@style/NoActionBarTheme" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/NoActionBarTheme" />
        <!--
        <service
